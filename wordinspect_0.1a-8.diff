--- wordinspect-0.1a.orig/debian/menu
+++ wordinspect-0.1a/debian/menu
@@ -0,0 +1,4 @@
+?package(wordinspect):needs="x11" section="Apps/Text" \
+	title="Dict Client" \
+	longtitle="GTK-based Dictionary Client" \
+	command="/usr/bin/wordinspect"
--- wordinspect-0.1a.orig/debian/postinst
+++ wordinspect-0.1a/debian/postinst
@@ -0,0 +1,27 @@
+#! /bin/sh
+
+set -e
+
+PACKAGE=wordinspect
+
+case "$1" in 
+     configure)
+      if [ -d /usr/doc -a ! -e /usr/doc/$PACKAGE -a -d /usr/share/doc/$PACKAGE ]; then
+              ln -sf ../share/doc/$PACKAGE /usr/doc/$PACKAGE
+      fi
+
+    test -x /usr/bin/update-menus && /usr/bin/update-menus || /bin/true
+
+    exit 0
+    ;;
+
+
+    abort-upgrade|abort-remove|abort-deconfigure|in-favour|removing)
+        exit 0;
+    ;;
+
+    *)
+        echo "postinst called with unknown argument \`$1'" >&2;
+        exit 0;
+    ;;
+esac
--- wordinspect-0.1a.orig/debian/postrm
+++ wordinspect-0.1a/debian/postrm
@@ -0,0 +1,5 @@
+#! /bin/sh
+
+set -e
+
+test -x /usr/bin/update-menus && /usr/bin/update-menus || /bin/true
--- wordinspect-0.1a.orig/debian/prerm
+++ wordinspect-0.1a/debian/prerm
@@ -0,0 +1,25 @@
+#! /bin/sh
+
+set -e
+
+PACKAGE=wordinspect
+
+case "$1" in
+    remove|purge|upgrade)
+if [ -L /usr/doc/$PACKAGE ]; then
+      rm -f /usr/doc/$PACKAGE
+fi
+        exit 0
+
+    ;;
+
+    abort-install|abort-upgrade|disappear)
+        exit 0
+    ;;
